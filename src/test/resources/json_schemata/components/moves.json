{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Moves",
  "description": "Moves by piece position",
  "type": "object",
  "additionalProperties": {
    "type": "array",
    "items": {
      "type": "string",
      "pattern": "^[A-Z][1-8](=[BNRQ])?$"
    },
    "uniqueItems": true
  },
  "propertyNames": {
    "pattern": "^[A-Z][1-8]$"
  }
}
